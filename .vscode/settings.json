{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "buble",
    "camelized",
    "consola",
    "kebabcase",
    "libundler",
    "pascalify",
    "rolluper",
    "surmon",
    "ubundler"
  ]
}
